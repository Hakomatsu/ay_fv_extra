<launch>
  <node type="fv_core_node" pkg="fingervision" name="fv_pi02" ns="fingervision" output="screen">
    <param name="pkg_dir" value="$(find ay_fv_extra)/" />
    <param name="cam_config" value="config/fv_pi02.yaml" />
    <param name="stereo_config" value="config/fv_pi02.yaml" />
    <param name="blobtrack_config" value="config/fv_pi02.yaml" />
    <param name="objdettrack_config" value="config/fv_pi02.yaml" />
    <param name="blob_calib_prefix" value="data_gen/blob_" />
    <param name="frame_skip" value="0" />
    <param name="target_fps" value="30" />
  </node>
</launch>
